<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" styleClass="bg" stylesheets="@styleSheet.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.fileController">
   <children>
      <TextField fx:id="tabName" layoutX="185.0" layoutY="89.0" prefHeight="30.0" prefWidth="200.0" styleClass="textfield" stylesheets="@styleSheet.css">
         <font>
            <Font name="Calibri" size="16.0" />
         </font></TextField>
      <TextField fx:id="nCluster" layoutX="517.0" layoutY="89.0" prefHeight="30.0" prefWidth="200.0" styleClass="textfield" stylesheets="@styleSheet.css">
         <font>
            <Font name="Calibri" size="16.0" />
         </font></TextField>
      <Label layoutX="186.0" layoutY="49.0" prefHeight="32.0" prefWidth="139.0" text="Nome tabella:" textFill="WHITE">
         <font>
            <Font name="Calibri" size="22.0" />
         </font>
      </Label>
      <Label layoutX="520.0" layoutY="52.0" prefHeight="27.0" prefWidth="150.0" text="Numero cluster:" textFill="WHITE">
         <font>
            <Font name="Calibri" size="22.0" />
         </font>
      </Label>
      <Button fx:id="load" layoutX="375.0" layoutY="143.0" mnemonicParsing="false" onAction="#loadFromFile" prefHeight="55.0" prefWidth="150.0" styleClass="button" stylesheets="@styleSheet.css" text="Carica File" textFill="WHITE">
         <font>
            <Font name="Calibri" size="22.0" />
         </font>
      </Button>
      <TextArea fx:id="result" cache="true" layoutX="29.0" layoutY="224.0" prefHeight="346.0" prefWidth="842.0" styleClass="textarea" stylesheets="@styleSheet.css">
         <font>
            <Font name="Calibri" size="15.0" />
         </font></TextArea>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backtoMenu" prefHeight="39.0" prefWidth="90.0" styleClass="button1" stylesheets="@styleSheet.css" text="Indietro" textFill="WHITE">
         <font>
            <Font name="Calibri" size="18.0" />
         </font></Button>
      <Text fx:id="info" fill="WHITE" layoutX="29.0" layoutY="211.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="209.0">
         <font>
            <Font name="Calibri Bold" size="14.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
